{
    "name": "atanvarno/http-util",
    "description": "PSR-7 utilities",
    "keywords": [
        "PSR-7", "utilities", "emitter"
    ],
    "homepage": "https://github.com/atanvarno69/http-util",
    "license": "MIT",
    "authors": [
        {
            "name": "atanvarno69",
            "email": "atanvarno69@gmail.com",
            "homepage": "https://github.com/atanvarno69",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "psr/http-message-implementation": "^1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0.0",
        "zendframework/zend-diactoros": "1.7.0"
    },
    "autoload": {
        "psr-4": {
            "Atanvarno\\Http\\Util\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Atanvarno\\Http\\Util\\Test\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
